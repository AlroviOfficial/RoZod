{
  "name": "rozod",
  "version": "2.0.0",
  "description": "Wrapper for Roblox API using Zod and custom API Client using fetch",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "generate": "node ./zodios-endpoints.js"
  },
  "author": "alexop1000",
  "license": "ISC",
  "dependencies": {
    "p-limit": "^3.0.0",
    "zod": "^3.21.4"
  },
  "repository": {
    "url": "https://github.com/alexop1000/RoZod"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@thepotato97/openapi-zod-client": "^1.6.4",
    "@types/jest": "^29.5.1",
    "@zodios/plugins": "^10.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  },
  "keywords": [],
  "files": [
    "lib/**/*"
  ]
}